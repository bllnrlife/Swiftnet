{
  "manifest_version": 3,
  "name": "Net Billionaire Search",
  "version": "1.2",
  "description": "Search directly from the address bar on net.billionaire.life",
  "permissions": ["storage", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "omnibox": {
    "keyword": "b"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
